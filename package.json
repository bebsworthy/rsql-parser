{
  "name": "@apsel/rsql-parser",
  "version": "0.0.5",
  "description": "A javascript library for parsing rsql expressions into predicates.",
  "main": "lib/rsql-parser.esm.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "regenerate": "./regenerate.sh",
    "test": "mocha",
    "push": "yarn build &&  yarn publish --non-interactive --no-git-tag-version --patch --access public --tag latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bebsworthy/rsql-parser.git"
  },
  "keywords": [
    "rsql",
    "predicate",
    "parser",
    "data",
    "expression",
    "language"
  ],
  "author": "pepper@vodori.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bebsworthy/rsql-parser/issues"
  },
  "homepage": "https://github.com/bebsworthy/rsql-parser#readme",
  "dependencies": {
    "antlr4": "4.8.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.1",
    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babelify": "8.0.0",
    "browserify": "16.5.0",
    "chai": "4.1.2",
    "minifyify": "7.3.5",
    "mocha": "7.0.0",
    "rollup": "^1.29.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-typescript2": "^0.25.3"
  }
}
